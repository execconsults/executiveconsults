    html:not(.tds-theme--dk) body:not(.i18n-model-ct) .summary-panel--footer {
      background: #aaa8a8;
    }

    .summary-panel--footer {
      border-radius: 16px 16px 0 0;
      grid-area: footer;
      margin: auto;
      padding: 0px 20px;
      position: relative;
      transform: translateY(0);
      transition: transform 1s var(--bezier) 0s;
    }

    .footer-modal-trigger {
      align-items: center;
      background: #fff;
      border-radius: 100%;
      display: flex;
      filter: drop-shadow(0 2px 2px rgba(0, 0, 0, .2));
      height: 32px;
      justify-content: center;
      min-width: 32px;
      overflow: visible;
      transition: filter .33s ease;
      width: 32px;
    }

    tds-icon--small {
      --tds-icon--size: 16px;
    }

    .tds-icon {
      background: var(--tds-icon--bg);
      block-size: var(--tds-icon--size);
      content: "";
      display: block;
      inline-size: var(--tds-icon--size);
      max-inline-size: 100%;
      min-block-size: var(--tds-icon--size);
      min-inline-size: var(--tds-icon--size);
      overflow: hidden;
      text-indent: -9999px;
      transition: color .33s ease, transform .5s var(--tds-bezier);
    }

    .summary-panel--footer .tds-flex-item {
      display: flex;
      margin: 0 10px;
    }

    .tds-flex-item {
      flex: 1;
    }

